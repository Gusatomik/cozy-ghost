  "engines": [
    "node = 0.10.x"
  ],
  "description": "Ghost blogging platform ported on cozy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cylwin/cozy-ghost.git"
  },
  "main": "server.js",
