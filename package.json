    "americano": "0.3.11",
    "cozydb": "0.0.3",
    "lodash": "^3.5.0"
  },
  "devDependencies": {
    "should": "2.0.1",
    "mocha": "1.13.0",
    "request-json": "0.4.4",
    "cozy-fixtures": "1.0.1",
    "jslint": "0.2.0"
  },
  "scripts": {
    "test": "cake tests",
    "start": "node server.js"
  },
  "cozy-permissions": {
    "Blog": {
      "description": "Creates and edits your blog"
